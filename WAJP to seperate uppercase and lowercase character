
public class Main
{
    public static void seperate(String s){
        String s1="";
        String s2="";
        for (int i=0;i<=s.length()-1;i++){
            if (s.charAt(i)>=65 && s.charAt(i)<=90){
                s1=s1+s.charAt(i);
            } 
            else if (s.charAt(i)>=97 && s.charAt(i)<=122){
                s2=s2+s.charAt(i);
            } 
        }
        System.out.println(s1+" "+s2);
    }
	public static void main(String[] args) {
        seperate("FUCKyou");
	}
}
