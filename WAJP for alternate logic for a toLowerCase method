
public class Main
{
    public static String toLower(String s){
        String str="";
        for (int i=0;i<=s.length()-1;i++){
            if (s.charAt(i)>=65 && s.charAt(i)<=90)
            str=str+(char)(s.charAt(i)+32);
            else
            str=str+s.charAt(i);
        } 
        return str;
    }
	public static void main(String[] args) {
		System.out.println(toLower("Hello World@123"));
	}
}
