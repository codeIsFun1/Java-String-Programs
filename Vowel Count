public class Main
{
    public static int count(String s){
        int c=0;
        s=s.toLowerCase();
        for (int i=0;i<=s.length()-1;i++){
            if( (s.charAt(i)=='a' || s.charAt(i)=='e' || s.charAt(i)=='i' || s.charAt(i)=='o' || s.charAt(i)=='u'))
            c++;
        }
        return c;
    }
	public static void main(String[] args) {
		System.out.println(count("AUTHENTICATION"));
	}
}
