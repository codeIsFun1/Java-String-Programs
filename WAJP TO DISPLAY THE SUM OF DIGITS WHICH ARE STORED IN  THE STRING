
public class Main
{
    public static int sum(String s){
        int sum=0;
        for (int i=0;i<=s.length()-1;i++){
            if (s.charAt(i)>=48 && s.charAt(i)<=57) {
                sum=sum+(s.charAt(i)-48);
            }
        } 
        return sum;
    }
	public static void main(String[] args) {
		System.out.println(sum("12345"));
	}
}
