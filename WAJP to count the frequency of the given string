public class Main
{
    public static int frequency(String s,char c){
        int c1=0;s=s.toLowerCase();
        for (int i=0;i<=s.length()-1;i++){
                if (s.charAt(i)==c)
                c1++;
        } 
        return c1;
    }
	public static void main(String[] args) {
		System.out.println(frequency("HELLO WORLD",'o'));
	}
}
